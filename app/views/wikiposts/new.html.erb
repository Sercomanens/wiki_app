<h1>New wikipost</h1>
<br>
<%= form_with model: @wikipost, local: true do |form| %>

<div>
  <%= form.label :title %>
  <%= form.text_field :title %>
  <%= form.label :description %>
  <%= form.text_field :description %>
  <%= form.label :author %>
  <%= form.text_field :author %>
   <%= form.file_field :image %>
</div>
<div>
  <%= form.submit 'Create' %>
</div>
<br>
<div>
  <%= link_to "Back to the wiki posts", wikiposts_path %>
</div>
<% end %>
